{
  "name": "date-namer",
  "version": "0.1.1",
  "main": "main.js",
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . date-namer --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Date Namer\"",
    "package-linux": "electron-packager . date-namer --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds" ,
    "start": "electron ."
  },
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-packager": "^12.1.2"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "moment": "^2.22.2"
  }
}
